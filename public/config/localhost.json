{
    "key": "localhost",
    "title": "本地的服务",
    "engine": "default",
    "mockDir": "/mockInfo",
    "states": {
        "prod": "http://localhost:3000",
        "test": "http://localhost:3000",
        "dev": "http://localhost:3000",
        "stag": "http://localhost:3000"
    },
    "state": "dev",
    "interfaces": [{
        "key": "home",
        "title": "主页接口",
        "method": "GET",
        "path": "/"
    }, {
        "key": "config",
        "title": "获取CONFIG信息",
        "method": "GET",
        "path": "/config"
    }, {
        "key": "test",
        "title": "获取测试数据",
        "method": "GET",
        "path": "/test"
    }, {
        "key": "test-server",
        "title": "获取测试数据",
        "method": "GET",
        "path": "/test/server"
    }, {
        "key": "login",
        "title": "测试登陆，返回数据",
        "method": "POST",
        "path": "/login",
        "dataSchema": {
            "type": "object",
            "required": ["username", "password"],
            "properties": {
                "username": {
                    "type": "string",
                    "maxLength": 20,
                    "minLength": 4
                },
                "password": {
                    "type": "string",
                    "minLength": 6
                }
            }
        }
    }]
}